{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "source": "/api/(.*)",
            "dest": "src/index.ts"
        },
        {
            "handle": "filesystem"
        },
        {
            "source": "/(.*)",
            "dest": "/index.html"
        }
    ]
}