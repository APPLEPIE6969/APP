{
    "version": 2,
    "builds": [
        {
            "src": "src/vercel.ts",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/src/vercel.ts"
        }
    ]
}